{
  "plugins": "./plugins",
  "web": {
    "title": "Verdaccio"
  },
  "uplinks": {
    "npmjs": {
      "url": "https://registry.npmjs.org/",
      "max_fails": 6
    }
  },
  "packages": {
    "**": {
      "access": "$all",
      "publish": "$all",
      "unpublish": "$all",
      "proxy": [
        "npmjs"
      ]
    }
  },
  "server": {
    "keepAliveTimeout": 60
  },
  "middlewares": {
    "audit": {
      "enabled": true
    }
  }
}
